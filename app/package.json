{
    "name": "express-ts-docker",
    "version": "1.0.0",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc",
        "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
        "migrate": "sequelize-cli db:migrate",
        "seed": "sequelize-cli db:seed:all",
        "seed:undo": "sequelize-cli db:seed:undo:all",
        "seed:reset": "sequelize-cli db:seed:undo:all && sequelize-cli db:seed:all",
        "model-auto:generate": "npx sequelize-auto -h db -d mydb -u user -x password -p 5432 --dialect postgres -o ./models --case l"
    },
    "dependencies": {
        "bcryptjs": "^3.0.3",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.3",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.16.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.37.7",
        "sequelize-auto": "^0.8.8",
        "sequelize-cli": "^6.6.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.7.0",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2"
    }
}
